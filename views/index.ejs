    <body>
      <h1>Welcome To Your Budgtr</h1>
      <div>  
        <table>
            <% for(let i=0; i< allBudget.length; i++) { %>
            <tr>
                <td><%= allBudget[i].date %></td>
                <td><a href="/budget/<%=i %>" ><%= allBudget[i].name %></a></td>
                <td><%= allBudget[i].amount %></td>
            </tr>
            <% } %>
        </table>
        <button><a href ="/budgets/new">Add Another Expense</a></button>
      </div>
    </body>
</html>